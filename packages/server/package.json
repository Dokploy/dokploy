    "@types/js-yaml": "4.0.9",
    "@types/lodash": "4.17.4",
    "@types/micromatch": "4.0.9",
    "@types/node": "^18.19.104",
    "@types/node-os-utils": "1.3.4",
    "@types/node-schedule": "2.1.6",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/ssh2": "1.15.1",
    "@types/ws": "8.5.10",
    "drizzle-kit": "^0.30.6",
    "esbuild": "0.20.2",
    "esbuild-plugin-alias": "0.2.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
