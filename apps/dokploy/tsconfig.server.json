{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"module": "ESNext",
		"outDir": "dist/",
		"target": "ESNext",
		"isolatedModules": false,
		"noEmit": false,
		"moduleResolution": "Node",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": ["next-env.d.ts", "./server/**/*"]
}
